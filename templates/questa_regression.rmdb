      <command>/usr/lib/sendmail  (%EMAIL_RECIPIENT%) &lt; $EMAILMESSAGE                                            </command>

      <command>echo "Regression complete and email report sent to (%EMAIL_RECIPIENT%)"                              </command>
    </execScript>
