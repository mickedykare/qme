      <parameter name="EXTRA_OPT_ARGS" type="tcl">[get_extra_args (%TCSPEC%) (%ACTUAL_SETUP%) vopt] </parameter>
      <parameter name="EXTRA_X_COMP_ARGS" type="tcl">[get_extra_args (%TCSPEC%) (%ACTUAL_SETUP%) X_COMP_ARGS]</parameter>
      <parameter name="EXTRA_SC_ARGS"    type="tcl">[get_extra_args (%TCSPEC%) (%ACTUAL_SETUP%) SC_ARGS]</parameter>
      <parameter name="ALL_COMPILE_ARGS">X_COMP_ARGS=&quot; (%EXTRA_X_COMP_ARGS%) --define FLTVAR_COMPILE_SETUP=(%ACTUAL_SETUP%) (%EXTRA_VLOG_ARGS%) (%EXTRA_VCOM_ARGS%) &quot; SC_ARGS=(%EXTRA_SC_ARGS%)</parameter>
      <parameter name="PRESCRIPT_TARGET" type="tcl">[check_if_target_exists (%RUNNABLE%)_preScript]</parameter>
      <parameter name="POSTSCRIPT_TARGET" type="tcl">[check_if_target_exists (%RUNNABLE%)_postScript]</parameter>
    </parameters>
    <execScript launch="/bin/csh -f">
      <command>unsetenv MODELSIM                                                                                                           </command>
      <command> if  ("(%PRESCRIPT_TARGET%)" == "1") then                                                                                   </command>
      <command>   echo "Executing target (%RUNNABLE%)_preScript"                                                                          </command>
      <command>   make -f (%MAKEFILE%) (%RUNNABLE%)_preScript REGRESSION=1  SETUP=(%ACTUAL_SETUP%)                                        </command>
      <command>   if ($status != 0) exit 1                                                                                                 </command>    
      <command> endif                                                                                                                      </command>
      <command> make compile -f (%MAKEFILE%)  VERIFICATION_LEVEL=(%VERIFICATION_LEVEL%) (%ALL_COMPILE_ARGS%) NOCOLOR=--nocolor BITS=(%BITS%) SETUP=(%ACTUAL_SETUP%)</command> 
      <command> if ($status != 0) exit 1                                                                                                    </command>
      <command> echo "Starting to optimize"                                                                                                 </command>
      <command> make opt_fast -f (%MAKEFILE%) VERIFICATION_LEVEL=(%VERIFICATION_LEVEL%) X_OPT_ARGS=(%EXTRA_OPT_ARGS%)  BITS=(%BITS%) SETUP=(%ACTUAL_SETUP%) COVER=1       </command>
      <command> if ($status != 0) exit 1                                                                                                    </command>
      <command> if ((%RERUN_FAILING_TESTS%) == 1) then                                                                                      </command>
      <command>   make opt_dbg -f (%MAKEFILE%) VERIFICATION_LEVEL=(%VERIFICATION_LEVEL%) X_OPT_ARGS=(%EXTRA_OPT_ARGS%)  BITS=(%BITS%) SETUP=(%ACTUAL_SETUP%) CCOVER=1      </command>
      <command>   if ($status != 0) exit 1                                                                                                  </command>
      <command> endif                                                                                                                       </command>
      <command> if  ("(%POSTSCRIPT_TARGET%)" == "1") then                                                                                   </command>
      <command>   echo "Executing target (%RUNNABLE%)_postScript"                                                                           </command>
      <command>   make -f (%MAKEFILE%) (%RUNNABLE%)_postScript REGRESSION=1    SETUP=(%ACTUAL_SETUP%)                                       </command>
      <command>   if ($status != 0) exit 1                                                                                                  </command>    
      <command> endif                                                                                                                       </command>
    </execScript>
  </runnable>

  
  <!-- ================================================ -->
  <!-- == Simulate group Runnable                    == -->
  <!-- == This group is run in parallell. The number == -->  
  <!-- == of parallell jobs are controlled by the    == -->
  <!-- == switch -j <nbr>                            == -->
  <!-- ================================================ -->
  <runnable name="all_sims" type="group" sequential="no" >
    <parameters>
      <parameter name="TEST_SEEDS" type="tcl">[get_test_seeds (%TCSPEC%) (%ACTUAL_SETUP%) (%test.ITERATION%) (%SANITY%)]</parameter>
      <parameter name="PRESCRIPT_TARGET" type="tcl">[check_if_target_exists (%RUNNABLE%)_preScript]</parameter>
      <parameter name="POSTSCRIPT_TARGET" type="tcl">[check_if_target_exists (%RUNNABLE%)_postScript]</parameter>
    </parameters>
    <preScript launch="/bin/csh -f">
      <command> if  ("(%PRESCRIPT_TARGET%)" == "1") then                                          </command>
      <command>   echo PRESCRIPT_TARGET=(%PRESCRIPT_TARGET%)                                      </command>
      <command>   echo "Executing target (%RUNNABLE%)_(%SCRIPT%)"                                 </command>
      <command>   make -f (%MAKEFILE%) (%RUNNABLE%)_(%SCRIPT%) REGRESSION=1 SETUP=(%ACTUAL_SETUP%)</command>
      <command>   if ($status != 0) exit 1                                                        </command>    
      <command> endif </command>
    </preScript>
    <postScript launch="/bin/csh -f">
      <command> if  ("(%POSTSCRIPT_TARGET%)" == "1") then                                         </command>
      <command>   echo POSTSCRIPT_TARGET=(%POSTSCRIPT_TARGET%)                                    </command>
      <command>   echo "Executing target (%RUNNABLE%)_(%SCRIPT%)"                                 </command>
      <command>   make -f (%MAKEFILE%) (%RUNNABLE%)_(%SCRIPT%) REGRESSION=1 SETUP=(%ACTUAL_SETUP%)</command>
      <command>   if ($status != 0) exit 1                                                        </command>    
      <command> endif</command>
    </postScript>
    <members>
      <member>Simulate</member>
      <member>formal</member>
      <member>autocheck</member>
      <member>xprop</member>
    </members>
  </runnable>

